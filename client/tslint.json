{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-standard",
        "tslint-react",
        "tslint-config-prettier",
        "tslint-react-hooks"
    ],
    "jsRules": {},
    "rules": {
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "member-ordering": false,
        "jsx-no-lambda": false,
        "jsx-boolean-value": false,
        "react-hooks-nesting": "error"
    },
    "rulesDirectory": []
}